package experimental;

import java.util.ArrayList;
import java.util.Random;

public class ex 
{
	public ex()
	{	
		ArrayList arr1 = new ArrayList();
		ArrayList arr2 = new ArrayList();
		for(int i = 0; i < 3; i++)
		{
			
		}
			
	}
	
	void merge(ArrayList<Integer> list1, ArrayList<Integer> list2)
	{
		ArrayList<Integer> _list1 = list1;
		ArrayList<Integer> _list2 = list2;
		ArrayList<Integer> _mergedArrayList = new ArrayList();
		int mSize = _list1.size() + _list2.size();
		
		_mergedArrayList.ensureCapacity(mSize);
		
		
		for(int l1Size = 0, l2Size = 0, count = 0; l1Size == _list1.size() && l2Size == _list2.size(); count++)
		{
			if(_list1.get(l1Size) >= _list2.get(l1Size))
			{
				_mergedArrayList.set(count, _list1.get(l1Size));
				l1Size++;
			}
			else if (_list1.get(l1Size) < _list2.get(l1Size))
			{
				_mergedArrayList.set(count, _list2.get(l1Size));
				l2Size++;
			}
		}
		
		list1 = _mergedArrayList;
	}
}

